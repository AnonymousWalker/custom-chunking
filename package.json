{
  "name": "custom-chunking",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "node index.js",
    "test": "node test.js",
    "electron": "electron main.js",
    "package-mac": "electron-packager . custom-chunking --overwrite --platform=darwin --arch=arm64 --prune=true --out=release-builds",
    "package-win": "electron-packager . custom-chunking --overwrite --asar=true --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=WA --version-string.FileDescription=WA --version-string.ProductName=custom-chunking",
    "package-linux": "electron-packager . custom-chunking --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnonymousWalker/custom-chunking.git"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.0",
    "pug": "^3.0.2",
    "yaml": "^2.2.1",
    "door43-client": "^0.9.6"
  },
  "devDependencies": {
    "electron": "^22.0.0",
    "electron-packager": "^17.1.1",
    "electron-rebuild": "^3.2.9",
    "node-gyp": "^9.3.1"
  }
}
